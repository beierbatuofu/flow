{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {

    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "erasableSyntaxOnly": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "baseUrl": ".",
    "paths":{
      "@src/*": ["src/*"],
   
    }
  },
  "include": ["src/*.d.ts","src/**/*.ts", "src/**/*.tsx", "src/**/*.vue",]
}
